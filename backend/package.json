{
  "name": "datadao-backend",
  "version": "1.0.0",
  "description": "Data DAO Backend API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build:prod": "tsc && npm run copy:assets",
    "copy:assets": "cp -r prisma dist/",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@liskhq/lisk-api-client": "^6.0.2",
    "@liskhq/lisk-client": "^6.0.2",
    "@liskhq/lisk-cryptography": "^4.1.0",
    "@liskhq/lisk-transactions": "^6.1.0",
    "@prisma/client": "^6.16.2",
    "bullmq": "^5.58.5",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "starknet": "^6.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.0",
    "prisma": "^6.16.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
