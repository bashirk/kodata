# Production Environment Variables
# Copy this to .env and fill in actual values

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/datadao_prod"

# Server
PORT=3001
NODE_ENV=production

# Lisk Network (Testnet)
LISK_BACKEND_PUBKEY="your_lisk_backend_public_key_hex"
LISK_BACKEND_PRIVKEY="your_lisk_backend_private_key_hex"

# Starknet Network (Testnet)
STARKNET_RPC_URL="https://starknet-testnet.public.blastapi.io"
STARKNET_ACCOUNT_ADDRESS="your_starknet_account_address"
STARKNET_PRIVATE_KEY="your_starknet_private_key"
STARKNET_CONTRACT_ADDRESS="your_starknet_contract_address"

# Redis (for BullMQ)
REDIS_URL="redis://localhost:6379"

# Security
JWT_SECRET="your_jwt_secret"
NEXTAUTH_SECRET="your_nextauth_secret"